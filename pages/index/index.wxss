/**index.wxss**/
.swipercss{width: 100%;height:380rpx;}
.swipercss image{width:750rpx;height:380rpx;}
.navlist{overflow: hidden;}
.navlist .libox{width:25%;float: left;padding: 30rpx;box-sizing:border-box;}
.navlist .libox .pic{width:50rpx;height:50rpx;margin: 0 auto}
.navlist .libox text{display:block;height: 50rpx;line-height: 50rpx;font-size: 24rpx;text-align: center;margin-top: 10rpx;color:#464c56}

.recommendlist .h2css{height:80rpx;width: 192rpx;background: url(http://www.ftde360.com/ytweb/chanpintuijian.png) bottom center no-repeat;margin:20rpx auto;text-align: center;padding-left:20rpx;}
.recommendlist .h2css text{font-size: 32rpx;color:#464c56;font-weight: bold; letter-spacing:16rpx;display:block;margin-bottom: 25rpx;height:40rpx;text-align: center}
.recommendlist .probox{overflow: hidden;padding:20rpx;}
.recommendlist .libox{width:50%;float: left;box-sizing:border-box;padding: 10rpx;overflow: hidden}
.recommendlist .libox .pic{width:100%;height:0;padding-bottom: 100%;background-color:#000000;overflow: hidden}
.recommendlist .libox button{width: 100%;overflow: hidden;display:block;border:none; background: none}
.recommendlist .libox button:after{display: none;}
.recommendlist .libox text{text-align: center;font-size:30rpx;display: block;height: 66rpx;line-height: 86rpx;color:#464c56;}